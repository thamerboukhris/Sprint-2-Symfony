<form method="post" action="{{ path('app_categories_delete', {'idCategorie': category.idCategorie}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ category.idCategorie) }}">
    <button    class="btn btn-outline-danger"  class="btn">Supprimer</button>

</form>
